[Tasks]
Name: "desktopicon"; Description: "{cm:CreateDesktopIcon}"; GroupDescription: "{cm:AdditionalIcons}"; Flags: unchecked

[Files]
Source: "D:\Users\semjo\Desktop\GO\projects\Pair Generator\Pair_Generator.exe"; DestDir: "{app}"; Flags: ignoreversion
