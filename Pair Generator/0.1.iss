Source: "D:\Users\semjo\Desktop\GO\projects\Pair Generator\files\*"; DestDir: "{app}\files\"; Flags: ignoreversion recursesubdirs createallsubdirs
Source: "D:\Users\semjo\Desktop\GO\projects\Pair Generator\Input\*"; DestDir: "{app}\Input\"; Flags: ignoreversion
Source: "D:\Users\semjo\Desktop\GO\projects\Pair Generator\languages\*"; DestDir: "{app}\languages\"; Flags: ignoreversion recursesubdirs createallsubdirs
Source: "D:\Users\semjo\Desktop\GO\projects\Pair Generator\Output\*"; DestDir: "{app}\Output\"; Flags: ignoreversion
; NOTE: Don't use "Flags: ignoreversion" on any shared system files

[Icons]
Name: "{autoprograms}\{#MyAppName}"; Filename: "{app}\{#MyAppExeName}"
Name: "{autodesktop}\{#MyAppName}"; Filename: "{app}\{#MyAppExeName}"; Tasks: desktopicon

